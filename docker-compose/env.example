# copy this file to the same folder as docker-compose and rename to .env
# amend the details below to suit your enviroment

# the path to the folder that contains your workspace
FILES_PATH = ""

# drupal version
DRUPAL_VERSION = ""

# the local (and container) name of the directory containing your Drupal files - in the local /drupal directory and container /var/www/ directory
DRUPAL_SITE_FILES = ""

# the root pass word for your mysql instance
DB_ROOT_PW = ""

 # the below must match those that are stated in your settings.php file
DRUPAL_DB = ""
DRUPAL_USER = ""
DRUPAL_PW = ""

# list of site urls, must be comma seperated and within `` marks
# eg SITES = "`site1`, `site2`"